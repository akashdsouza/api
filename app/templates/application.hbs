<div id="header">
  <div id="wrapper">
    <a id="logo" href="/">&nbsp;</a>
    <ul id="nav">
      <li><a href="/about">about</a></li>
      <li><a href="/guides">guides</a></li>
      <li class="active"><a href="/api">api</a></li>
      <li><a href="/community">community</a></li>
      <li><a href="/blog">blog</a></li>
      <li><a href="/builds">builds</a></li>
      <li id="search">
        <form>
          <input type="text" id="st-search-input" class="st-search-input" autocomplete="off" autocorrect="off" autocapitalize="off" style="outline: none;">
        </form>
        <div id="st-results-container"></div>
      </li>
    </ul>

    <div id="github">
      <a href="https://github.com/emberjs/ember.js"><i class="icon-fork"></i>Fork Us!</a>
    </div>
  </div>
</div>


<div id="content-wrapper">
  <div id="sidebar">
    <ol id="toc-list">
      {{#if model.libraries}}
        <li class="level-1">
          <a href="#">Projects</a>
          <ol>
            {{#each model.libraries as |library|}}
             <li class="level-3">
                <a href="{{library.slug}}">{{library.name}}</a>
              </li>
            {{/each}}
          </ol>
        </li>
      {{/if}}

      <li class="level-1">
        <a href="{{githubHREF}}" target="_blank"> Tag: {{rev}}</a>
        {{#if model.versions}}
          {{#power-select class="select-version" options=model.versions selected=rev onchange=(action "versionChanged") as |version|}}
            {{version}}
          {{/power-select}}
        {{/if}}
      </li>

      <li class="level-1">
        <a href="#">Modules</a>
        <ol>
          {{#each model.modules as |module|}}
            <li class='level-3'>
              {{#link-to 'module' module.name}}
                {{module.name}}
              {{/link-to}}
            </li>
          {{/each}}
        </ol>
      </li>
      <li class="level-1 ">
        <a href="#">Namespaces</a>
        <ol>
          {{#each model.namespaces as |namespace|}}
            <li class='level-3'>
              {{#link-to 'klass' namespace.name}}
                {{namespace.name}}
              {{/link-to}}
            </li>
          {{/each}}
        </ol>
      </li>

      <li class="level-1 ">
        <a href="#">Classes</a>
        <ol class="">
          {{#each model.classes as |klass|}}
            <li class='level-3'>
              {{#link-to 'klass' klass.name}}
                {{klass.name}}
              {{/link-to}}
            </li>
          {{/each}}
        </ol>
      </li>

      {{#if view.returnToTopLinkVisible}}
        <div id="back-to-top">
          <a href="#" {{action "scrollToTop" target=view}}>â¬† Back to Top</a>
        </div>
      {{/if}}
    </ol>
  </div>

  <div id="content" class="has-sidebar">
    {{outlet}}
  </div>
</div>


